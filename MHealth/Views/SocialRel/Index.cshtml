@model IEnumerable<DAL.Models.mp_social_relationship>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.num_marriages)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.relationship_concern)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.social_activities)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.support_network)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.created_at)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.created_by)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.profile_)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.num_marriages)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.relationship_concern)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.social_activities)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.support_network)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.created_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.created_by)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.profile_.first_name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
