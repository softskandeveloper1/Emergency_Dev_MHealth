@section scripts{
    <!-- Javascript -->
    <script src="~/front/js/jquery-1.11.1.min.js"></script>
    <script src="~/front/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/front/js/jquery.backstretch.min.js"></script>
